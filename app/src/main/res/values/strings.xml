        <attr name="vive_et_cursor_height" format="dimension" />
        <!--输入框焦点颜色-->
        <attr name="vive_et_cursor_color" format="color" />
        <!--输入框下划线高度-->
        <attr name="vive_et_underline_height" format="dimension" />
        <!--输入框无焦点的下划线颜色-->
        <attr name="vive_et_underline_default_color" format="color" />
        <!--输入框有焦点的下划线颜色-->
        <attr name="vive_et_underline_focus_color" format="color" />
        <!--输入框下划线是否展示-->
        <attr name="vive_et_underline_show" format="boolean" />
    </declare-styleable>
